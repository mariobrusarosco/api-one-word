{
  "name": "api-one-word",
  "version": "0.2.3",
  "description": "API - One Word",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./dist && yarn generate-schema && tsc",
    "dev": "nodemon -r dotenv/config ./src/index.ts",
    "start": "node -r dotenv/config ./dist/src/index.js",
    "compile": "tsc",
    "studio": "prisma studio --schema=./src/prisma/schema.prisma",
    "generate-schema": "prisma generate --schema=./src/prisma/schema.prisma",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org mario-79 --project one-word ./dist && sentry-cli sourcemaps upload --org mario-79 --project one-word ./dist"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "author": "Mario Brusarosco",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mariobrusarosco/api-one-word.git"
  },
  "bugs": {
    "url": "https://github.com/mariobrusarosco/api-one-word/issues"
  },
  "homepage": "https://github.com/mariobrusarosco/api-one-word/#readme",
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "@sentry/cli": "^2.31.2",
    "@sentry/node": "^8.0.0",
    "@sentry/profiling-node": "^8.0.0",
    "@socket.io/admin-ui": "^0.5.1",
    "@types/mongoose": "^5.11.97",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "jsonwebtoken": "^9.0.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.14.0",
    "socket.io": "^4.7.2",
    "typescript": "^5.3.2",
    "uuid": "^9.0.1",
    "ws": "^8.15.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.10.4",
    "@types/uuid": "^9.0.6",
    "@types/ws": "^8.5.10",
    "hbs": "^4.2.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  }
}
